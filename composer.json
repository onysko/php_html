{
    "name":"samsonos/php_html",
    "description": "SamsonPHP html markup generator module",
    "keywords": [ "markup", "generator", "html", "frontend", "samsonphp" ],
    "authors":[
        {
            "name":"Vitaly Iegorov",
            "email":"egorov@samsonos.com"
        }
    ],
    "license":"Open Software License (OSL) v 3.0",
    "homepage":"http://samsonphp.com/",
    "autoload" : {
        "classmap": ["src/"],
        "psr-0": {
            "samsonos\\html\\": "src/",
            "samson\\html\\": "src/"
        }
    },
    "require": {
        "samsonos/php_core": "1.*",
        "samsonos/php_fs": "1.*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "squizlabs/php_codesniffer": "2.0.*@dev",
        "phpunit/phpunit": "4.2.*"
    }
}
